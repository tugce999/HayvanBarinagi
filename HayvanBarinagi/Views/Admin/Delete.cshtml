@model CreateViewModel
<form method="post">
    <input asp-for="Id" hidden/>
    <div class="border p-3 mt-4">
        <div class="row pb-2">
            <h2 class="text-primary">Delete User</h2>
            <hr />
        </div>
        @*<div asp-validation-summary="ModelOnly"></div>*@
        <div class="mb-3 row p-1">
            <label asp-for="Id" class="p-0"></label>
            <input asp-for="Id" class="form-control" />
           
        </div>
        <div class="mb-3 row p-1">
            <label asp-for="NameSurname" class="p-0"></label>
            <input asp-for="NameSurname" disabled class="form-control" />
            
        </div>
        <div class="mb-3 row p-1">
            <label asp-for="UserName" class="p-0"></label>
            <input asp-for="UserName" disabled class="form-control" />
            
        </div>
        <div class="mb-3 row p-1">
            <label asp-for="CreatedAdd " class="p-0"></label>
            <input asp-for="CreatedAdd " disabled class="form-control" />
           
        </div>
        <div class="mb-3 row p-1">
            <label asp-for="Role" class="p-0"></label>
            <input asp-for="Role" disabled class="form-control" />
            
        <div class="row">
            <div class="col-6 col-md-3">
                <button type="submit" class="btn btn-primary form-control">Update</button>
            </div>
            <div class="col-6 col-md-3">
                <a asp-controller="Admin" asp-action="Index" class="btn btn-secondary border  form-control">
                    Back to List
                </a>
            </div>
        </div>


    </div>
</form>
